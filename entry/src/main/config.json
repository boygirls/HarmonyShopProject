{
  "app": {
    "bundleName": "com.shop.x",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {
    "default": {
      "keepAlive": true,
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "package": "com.shop.x",
    "name": ".MyApplication",
    "mainAbility": "com.shop.x.TabList",
    "reqPermissions": [
      {
        "name": "ohos.permission.GET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.SET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.INTERNET",
        "reason": "access internet",
        "usedScene": {
          "ability": [
            "com.shop.x.LoginAbility"
          ],
          "when": "always"
        }
      }
    ],
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "name": "com.shop.x.TabList",
        "description": "$string:tablist_description",
        "icon": "$media:icon",
        "label": "$string:entry_TabList",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "name": "com.shop.x.WelcomeAbility",
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "description": "$string:welcomeability_description",
        "icon": "$media:icon",
        "label": "$string:entry_WelcomeAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "name": "com.shop.x.LoginAbility",
        "description": "$string:loginability_description",
        "icon": "$media:icon",
        "label": "$string:entry_LoginAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "name": "com.shop.x.MallDataAbility",
        "description": "$string:malldataability_description",
        "type": "data",
        "uri": "dataability://com.shop.MallDataAbility",
        "icon": "$media:icon"
      },
      {
        "name": "com.shop.x.AddAbility",
        "description": "$string:addability_description",
        "icon": "$media:icon",
        "label": "$string:entry_AddAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "skills": [
          {
            "actions": [
              "com.huawei.push.action.MESSAGING_EVENT"
            ]
          }
        ],
        "name": "com.shop.x.push.HmsMessageServiceAbility",
        "description": "$string:hmsmessageserviceability_description",
        "type": "service",
        "backgroundModes": [],
        "icon": "$media:icon"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.Light.NoTitleBar"
        },
        {
          "name": "com.huawei.hms.client.appid",
          "value": "107244179"
        }
      ]
    },
    "defPermissions": [
      {
        "name": "com.shop.DataAbilityShellProvider.PROVIDER"
      }
    ]
  }
}