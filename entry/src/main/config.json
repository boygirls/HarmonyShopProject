{
  "app": {
    "bundleName": "com.shop",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },

  "deviceConfig": {
    "default": {
      "keepAlive": true,
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "package": "com.shop",
    "name": ".MyApplication",
    "mainAbility": "com.shop.TabList",
    "reqPermissions": [
      {
        "name": "ohos.permission.GET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.SET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.INTERNET"
      }
    ],
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "name": "com.shop.TabList",
        "description": "$string:tablist_description",
        "icon": "$media:icon",
        "label": "$string:entry_TabList",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "name": "com.shop.user.UserAbility",
        "description": "$string:userability_description",
        "icon": "$media:icon",
        "label": "$string:entry_UserAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "name": "com.shop.WelcomeAbility",
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "description": "$string:welcomeability_description",
        "icon": "$media:icon",
        "label": "$string:entry_WelcomeAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.Light.NoTitleBar"
        }
      ]
    }
  }
}